<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No se encontraron datos"
		title="{config>/title}"
		search="handleSearch"
		confirm="handleClose"
		close="handleClose"
		items="{
			path: 'data>/'
		}" >
		<StandardListItem
			key="{data>id_categoria}"
			title="{data>categoria}{data>marca}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>