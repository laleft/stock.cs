<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No se encontraron datos"
		title="{config/title}"
		search="handleSearch"
		confirm="handleClose"
		close="handleClose"
		items="{
			path: '/datos'
		}" >
		<StandardListItem
			title="{Name}"
			description="{ProductId}"
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>